{
  "name": "soundfont",
  "private": true,
  "version": "0.5.1",
  "description": "A soundfont player.",
  "main": "dist/main.js",
  "jsnext:main": "src/scripts/main.js",
  "module": "src/scripts/main.js",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint:css": "csscomb src/styles && stylelint src/styles --fix",
    "lint:js": "eslint src/scripts --fix",
    "lint": "npm run lint:css & npm run lint:js",
    "serve": "ELEVENTY_ENV=development npm run watch:site & NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "build:site": "eleventy --config eleventy.config.js",
    "build:bundle": "webpack --config webpack.prod.js",
    "prebuild": "npm run clean",
    "watch:site": "npm run build:site -- --watch",
    "build": "ELEVENTY_ENV=production npm run build:site & NODE_ENV=production npm run build:bundle",
    "test": "echo \"Error: no test specified \" && exit 1",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sccherry/soundfont.git"
  },
  "keywords": [
    "audio",
    "midi",
    "pwa"
  ],
  "author": "Steve Cherry <stevenccherry@gmail.com> (https://stevecherry.net/)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sccherry/soundfont/issues"
  },
  "homepage": "https://github.com/sccherry/soundfont#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "redux": "3.7.2",
    "zen-observable": "0.8.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "7.1.6",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "csscomb": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-compat": "2.1.0",
    "eslint-plugin-import": "2.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "11.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "4.0.0",
    "rimraf": "2.6.1",
    "style-loader": "^0.23.1",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-no-unsupported-browser-features": "1.0.1",
    "tailwindcss": "^1.0.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
