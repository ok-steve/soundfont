{
  "name": "soundfont",
  "private": true,
  "version": "0.5.1",
  "description": "A soundfont player.",
  "main": "dist/main.js",
  "jsnext:main": "src/scripts/main.js",
  "module": "src/scripts/main.js",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist && mkdirp dist/assets",
    "copy": "cpy '*.{ico,xml,json}' 'assets/*.{png,svg}' 'service-worker.js' '../dist' --cwd=src --parents",
    "serve": "npm run watch:html & NODE_ENV=development webpack-dev-server --content-base dist/ --port 3000",
    "build:html": "nunjucks *.njk data.json -p src -o dist",
    "build:app": "NODE_ENV=production webpack",
    "prebuild": "npm run clean && npm run copy",
    "watch:html": "onchange 'src/**/*.njk' -- npm run build:html",
    "build": "npm run build:html & npm run build:app",
    "test": "echo \"Error: no test specified \" && exit 1",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sccherry/soundfont.git"
  },
  "keywords": [
    "audio",
    "midi",
    "pwa"
  ],
  "author": "Steve Cherry <stevenccherry@gmail.com> (https://stevecherry.net/)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sccherry/soundfont/issues"
  },
  "homepage": "https://github.com/sccherry/soundfont#readme",
  "dependencies": {
    "redux": "3.7.2",
    "snabbdom": "0.6.9",
    "soundfont-player": "0.10.5",
    "tachyons": "4.8.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.6.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "3.0.0",
    "mkdirp": "0.5.1",
    "nunjucks-cli": "0.5.1",
    "onchange": "3.2.1",
    "rimraf": "2.6.1",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.6.1"
  }
}
