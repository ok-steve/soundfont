<label class="block text-xl font-bold mb-4" for="octave">Octave</label>
<select id="octave" class="w-full" name="octave">
  {% for option in [2, 3, 4, 5, 6, 7] %}
    <option value="{{ option }}"{% if option == initialState.octave %} selected{% endif %}>{{ option }}</option>
  {% endfor %}
</select>
